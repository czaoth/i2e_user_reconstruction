  Year                     = {2010},

  Address                  = {New York, {NY}, {USA}},
  Note                     = {00001},
  Pages                    = {311?318},
  Publisher                = {{ACM}},
  Series                   = {{VRCAI} '10},

  Abstract                 = {In this paper, a {2D} motion detection bounded hand {3D} trajectory tracking and gesture recognition system is proposed for virtual reality interactions. First, the Bayes decision rule for classification of background and foreground is utilized to automatically locate the hand that bounded within a rectangle, and then the trajectory of the hand in {3D} space is tracked by mean shift particle filter and stereo imaging. The skin color feature is exploited for image matting that effectively segment the hand contour in video sequence automatically. Finally the hand gesture is recognized by the connected component analysis and line approximation. The proposed technique works without any markers or constraints, overcomes the disturbance of arms and faces in the scene, and can recognize multiple hands with different gestures under complex background.},
  Doi                      = {10.1145/1900179.1900245},
  File                     = {ACM Full Text PDF:files/132/Wu et al. - 2010 - 2D motion detection bounded hand 3D trajectory tra.pdf:application/pdf},
  ISBN                     = {978-1-4503-0459-7},
  Keywords                 = {Bayes decision rule, gesture recognition, hand detection, image matting, line approximation, mean shift, particle filer, skin color, stereo imaging, virtual reality},
  Owner                    = {squad},
  Timestamp                = {2013.10.14},
  Url                      = {http://doi.acm.org/10.1145/1900179.1900245},
  Urldate                  = {2013-10-14}
}

