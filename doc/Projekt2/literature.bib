@InProceedings{wu_2d_2010,
  Title                    = {{2D} motion detection bounded hand {3D} trajectory tracking and gesture recognition under complex background},
  Author                   = {Wu, Shuangqing and Zhang, Yin and Zhang, Sanyuan and Ye, Xiuzi and Cai, Yiyu and Zheng, Jianmin and Ghosh, Soumita and Chen, Wenyu and Zhang, Jane},
